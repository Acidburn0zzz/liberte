# Encrypted storage parameters
OTFEFILE=/otfe/liberte.vol
OTFELABEL="Libert√© OTFE"

# Size, can be fraction of the free space on media (A or A/B)
OTFESIZE=1/4

# Encryption cipher + block mode, key size,
# and key setup / volume key digest hash
OTFECIPHER=aes-xts-plain64
OTFEKEYSIZE=512
OTFEHASH=sha512

# LVM partition name
# (also used by otfe-resize)
OTFEVOLUME=otfe

# Password entry attempts
OTFEPWDTRIES=10
