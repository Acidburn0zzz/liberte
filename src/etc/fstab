# /proc, /sys{,/kernel{/security,/debug}}, /dev{,/shm,/pts}
# are mounted via init.d boot services

# Control groups (mount just the cpu subsystem)
# Not using /sys/fs/cgroup due to GRKERNSEC_SYSFS_RESTRICT
cgroup   /var/cgroup     cgroup   cpu,nosuid,nodev,noexec  0 0

# Persistence mount is added below by /etc/init.d/liberte
