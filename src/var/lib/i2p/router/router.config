# NOTE: This I2P config file must use UTF-8 encoding

router.dynamicKeys=false
router.firstInstalled=1314053515816
router.firstVersion=0.8.7

# External clients support via I2CP is disabled
i2cp.SSL=false
i2cp.auth=false
i2cp.disableInterface=true
i2cp.hostname=127.0.0.1
i2cp.port=7654
i2cp.tcp.bindAllInterfaces=false

# Change selected ports for each IP change (including start-up)
i2np.laptopMode=true
i2np.ntcp.autoip=false
i2np.ntcp.enable=true
i2np.udp.addressSources=hidden
i2np.udp.internalPort=12345
i2np.udp.port=12345

# Don't open external ports and/or share traffic
router.isHidden=true
router.sharePercentage=30
i2np.upnp.enable=false

# Reseed via Tor
router.reseedProxyEnable=true
router.reseedProxyHost=127.0.0.1
router.reseedProxyPort=8118
router.reseedSSLDisable=true
router.reseedSSLRequired=false

# HTTPS seeds: https://a.netdb.i2p2.de/,https://c.netdb.i2p2.de/,https://forum.i2p2.de/netdb/,https://r31453.ovh.net/static_media/files/netDb/,https://www.i2pbote.net/netDb/
